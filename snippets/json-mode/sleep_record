"${1:`(format-time-string "%Y-%m-%d")`}" : {
  "заполнить утром(sleep)" : {
    "лечь в 23:00-00:00, встать в 08:00-09:00" : ${2:0},
    "когда пошел спать вчера" : "${3:0000}",
    "когда встал утром сегодня" : "${4:0000}",
    "ск-ко часов спал" : ${5:8},
    "сколько времени засыпал(мин)" : ${6:0},
    "какие медикаменты принимал, чтобы уснуть" : "${7:-}",
    "как чувствовал себя после пробуждения(1-4)" : ${8:0}
  },

  

},